<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
	<bean id="marks1" class="com.springproject.bean.autowire.xml.Marks">
	<property name="marks1" value="100"/>
	<property name="marks2" value="200"/>
	<property name="marks3" value="300"/>
	</bean>
	
	<bean id="student" class="com.springproject.bean.autowire.xml.Student" autowire="default">
	<property name="studetId" value="34"/>
	<property name="studentName" value="Ganesh"/>
	</bean>
	</beans>
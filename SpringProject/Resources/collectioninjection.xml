<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean name="organisation"
		class="com.springproject.di.settor.collections.Organisation">
		<property name="students">
			<list>
				<value>Suresh</value>
				<value>ganesh</value>
				<value>Girish</value>
			</list>
		</property>

		<property name="depart">
			<set>
				<value>CS</value>
				<value>IT</value>
				<value>Finance</value>
			</set>
		</property>

		<property name="staf">
			<map>
				<entry key="123" value="Kiran" />
				<entry key="45" value="Rajesh" />
			</map>
		</property>

		<property name="propert">
			<props>
				<prop key="CS Depart">100</prop>
				<prop key="IT Depart">200</prop>
				<prop key="Finance Depart">20</prop>
			</props>

		</property>

	</bean>

</beans>